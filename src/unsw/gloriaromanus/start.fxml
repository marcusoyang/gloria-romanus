                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                     </padding>
                  </TextField>
                  <TextField fx:id="numPlayers" layoutX="24.0" layoutY="279.0" prefWidth="185.0" promptText="Number of Players">
                     <padding>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                     </padding>
                  </TextField>
                  <VBox layoutX="16.0" layoutY="316.0" prefHeight="284.0" prefWidth="200.0">
