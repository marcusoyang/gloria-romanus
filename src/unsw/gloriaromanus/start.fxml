                              <Insets bottom="2.0" left="2.0" right="2.0" top="2.0" />
                           </VBox.margin>
                        </CheckBox>
                        <CheckBox fx:id="Carthaginians" mnemonicParsing="false" text="Carthaginians" textFill="WHITE">
