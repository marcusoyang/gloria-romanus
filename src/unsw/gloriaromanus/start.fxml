                           </VBox.margin>
                        </CheckBox>
                        <CheckBox fx:id="Gauls" mnemonicParsing="false" text="Gauls" textFill="WHITE">
                           <VBox.margin>
                              <Insets bottom="2.0" left="2.0" right="2.0" top="2.0" />
                           </VBox.margin>
                        </CheckBox>
                        <CheckBox fx:id="CelticBritons" mnemonicParsing="false" text="Celtic Britons" textFill="WHITE">
