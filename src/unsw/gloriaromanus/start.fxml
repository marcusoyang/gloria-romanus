<?import javafx.scene.paint.Color?>

<StackPane prefHeight="900.0" prefWidth="1600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <VBox maxWidth="250" spacing="10" styleClass="panel-region" StackPane.alignment="TOP_LEFT">
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
         <background>
            <Background>
                <fills>
                    <BackgroundFill>
                        <fill>
                            <Color opacity="0.3" />
                        </fill>
                    </BackgroundFill>
                </fills>
            </Background>
        </background>
        <children>
            <Pane prefHeight="900.0" prefWidth="200.0">
               <children>
                  <Button fx:id="startButton" alignment="CENTER" contentDisplay="CENTER" layoutX="24.0" layoutY="233.0" mnemonicParsing="false" onAction="#handleStartButton" prefHeight="46.0" prefWidth="185.0" text="New Game" />
