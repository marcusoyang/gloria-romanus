                        <CheckBox fx:id="Macedonians" mnemonicParsing="false" text="Macedonians" textFill="WHITE">
                           <VBox.margin>
                              <Insets bottom="2.0" left="2.0" right="2.0" top="2.0" />
                           </VBox.margin>
                        </CheckBox>
                        <CheckBox fx:id="Thracians" mnemonicParsing="false" text="Thracians" textFill="WHITE">
                           <VBox.margin>
                              <Insets bottom="2.0" left="2.0" right="2.0" top="2.0" />
                           </VBox.margin>
                        </CheckBox>
                        <CheckBox fx:id="Dacians" mnemonicParsing="false" text="Dacians" textFill="WHITE">
                           <VBox.margin>
                              <Insets bottom="2.0" left="2.0" right="2.0" top="2.0" />
